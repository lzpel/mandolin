{%- elif schema.type == "number" -%}
f64
{%- elif schema.type == "boolean" -%}
bool
{%- else -%}
{%- endif %}
{%- elif schema|ref -%}
{{schema|ref}}
{%- else -%}
u8
{%- endif %}
{%- endmacro SCHEMA -%}
{%- macro IDENTIFIED_SCHEMA(name, schema) -%}
