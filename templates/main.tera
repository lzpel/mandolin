pub struct {{name}}{
{{self::SCHEMA(schema=schema)}}
}
{%- else  -%}
pub type {{name}} = {{self::SCHEMA(schema=schema)}};
{%- endif -%}
{%- endmacro IDENTIFIED_SCHEMA -%}
